locations:
  - name: "frontend"
    location: "packages/frontend"
    type: "npm"
    commands:
      - "start"
      - "build"
      - "test"
  - name: "backend"
    location: "packages/backend"
    type: "go"
    commands:
      - "run"
      - "test"
  - location: "scripts"
    commands:
      - "deploy.sh"
      - "backup.sh"